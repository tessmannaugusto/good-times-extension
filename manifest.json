{
  "manifest_version": 3,
  "name": "good-times",
  "version": "1.0",
  "description": "dot not waste your precious time",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "good-times"
  },
  "options_page": "options/options.html",
  "options_ui": {
    "page": "options/options.html"
  },
  "icons": {
    "16": "icons/hourglass16-2.png",
    "48": "icons/hourglass48.png",
    "128": "icons/hourglass128.png"
  }
}